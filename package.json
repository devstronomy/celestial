  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "clean": "rm -rf dist",
    "flush-npm": "rm -rf node_modules && npm install",
    "lint": "eslint src/**/*.[jt]s",
    "lint:fix": "eslint --fix src/**/*.[jt]s",
    "start": "webpack serve --open --config webpack.dev.js",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:watch": "jest --watchAll",
    "prettier": "prettier -c src && prettier-package-json -l ./package.json",
    "prettier:fix": "prettier -w src && prettier-package-json --write ./package.json"
  },
  "dependencies": {
    "@devstronomy/canvas": "0.1.0"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.3.1",
    "eslint": "7.32.0",
    "html-webpack-plugin": "5.5.0",
    "import-sort-style-module": "6.0.0",
    "jest": "27.4.5",
    "mini-css-extract-plugin": "2.4.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "prettier-package-json": "2.6.0",
    "prettier-plugin-import-sort": "0.0.7",
    "style-loader": "3.3.1",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.2",
    "webpack-merge": "5.8.0"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "8.1.2"
  }
}
